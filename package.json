    "detect-node": "^2.0.3",
    "echarts": "^3.6.2",
    "element-theme": "0.7.2",
    "element-theme-default": "1.4.9",
    "element-ui": "1.4.9",
    "es6-promise": "^4.1.0",
    "html5-history-api": "^4.2.8",
    "js-cookie": "2.1.4",
    "lru-cache": "^4.1.1",
    "pingpp-js": "^2.1.8",
